<?xml version="1.0"?>
<config>
	<tabs>
        <anowave translate="label" module="price">
            <label>Anowave Extensions</label>
            <sort_order>150</sort_order>
        </anowave>
    </tabs> 
    <sections>
        <price translate="label" module="price">
            <label>Customer price configuration</label>
            <tab>anowave</tab>
            <frontend_type>text</frontend_type>
            <sort_order>980</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <settings>
                    <label>Settings</label>
                    <sort_order>10</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <expanded>1</expanded>
                    <fields>
                    	<license translate="label comment">
							<label>License key</label>
							<comment>
								<model>Anowave_Package_Model_License</model>
							</comment>
							<frontend_type>text</frontend_type>
							<sort_order>0</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
						</license>
                    	 <custom translate="label">
                            <label>Enable customer individual price</label>
                            <comment>Turn on/off price per customer. If enabled prices will be overriden with those from customer's settings.</comment>
                            <frontend_type>select</frontend_type>
                            <sort_order>-1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                        </custom>
                        <status translate="label">
                            <label>Disable price for guests</label>
                            <comment>Turn on/off price disable</comment>
                            <frontend_type>select</frontend_type>
                            <sort_order>90</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                        </status>
                        <message translate="label">
                            <label>Replace price with text:</label>
                            <comment>Substitute price on frontend with custom message</comment>
                            <frontend_type>text</frontend_type>
                            <sort_order>91</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </message>
                    </fields>
                </settings>
                <import>
                	<label>Import</label>
                    <sort_order>11</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <expanded>0</expanded>
                    <fields>
                    	<key translate="label">
					        <label>Key type</label>
					        <frontend_type>select</frontend_type>
					        <source_model>price/system_config_key</source_model>
					        <sort_order>1</sort_order>
					        <show_in_default>1</show_in_default>
					        <show_in_website>1</show_in_website>
					        <show_in_store>1</show_in_store>
					        <comment>Product ID column can contain either Product ID or SKU. Select the key type used in the imported CSV file.</comment>
					    </key>
	                	<file translate="label">
					        <label>Import price(s)</label>
					        <comment>
					        	<![CDATA[
					        		Upload a CSV file to import multiple price(s) <br /> <br />
					        		<img src="data:image/png;base64," />
					        		<br />
					        		<strong>Customer ID</strong> - 	Number. (Required) Customer ID as in Magento <br />
					        		<strong>Product ID</strong>	- Number. (Optional) Product ID as in Magento <br />
					        		<strong>Discount Value</strong>	- Number. (Required) <br />
					        		<strong>Discount Type</strong> - String. (Required). Accepted values: F - Fixed discount, P - Percent discount <br />
					        		<strong>Category</strong> - Number. (Optional)
					        	]]>
					        </comment>
					        <frontend_type>file</frontend_type>
					        <backend_model>adminhtml/system_config_backend_file</backend_model>
					        <upload_dir>var/uploads</upload_dir>
					        <sort_order>2</sort_order>
					        <show_in_default>1</show_in_default>
					        <show_in_website>1</show_in_website>
					        <show_in_store>1</show_in_store>
					    </file>
					    
					</fields>
                </import>
                <initial>
                	<label>Initial discount settings</label>
                    <sort_order>12</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <expanded>0</expanded>
                    <fields>
	                	<apply translate="label">
                            <label>Apply initial discount</label>
                            <comment>Apply initial discount for new customers</comment>
                            <frontend_type>select</frontend_type>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                        </apply>
                        <discount_type translate="label">
					        <label>Discount type</label>
					        <frontend_type>select</frontend_type>
					        <source_model>price/system_config_type</source_model>
					        <sort_order>2</sort_order>
					        <show_in_default>1</show_in_default>
					        <depends>
					        	<apply>1</apply>
					        </depends>
					        <comment>Select the type of discount to apply. Determines how discount is calculated.</comment>
					    </discount_type>
					    <dicount_fixed>
					    	<label>Fixed amount</label>
					        <frontend_type>text</frontend_type>
					        <sort_order>3</sort_order>
					        <show_in_default>1</show_in_default>
					        <depends>
					        	<discount_type>F</discount_type>
					        </depends>
					    </dicount_fixed>
					    <dicount_percentage>
					    	<label>Percentage amount</label>
					        <frontend_type>text</frontend_type>
					        <sort_order>4</sort_order>
					        <show_in_default>1</show_in_default>
					        <depends>
					        	<discount_type>P</discount_type>
					        </depends>
					    </dicount_percentage>
					    <discount_categories translate="label">
					        <label>Categories</label>
					        <frontend_type>text</frontend_type>
					        <sort_order>5</sort_order>
					        <show_in_default>1</show_in_default>
					        <depends>
					        	<apply>1</apply>
					        </depends>
					        <comment>A CSV separated string of category ID e.g. 1,10,12. Note that your categories ID may be different.</comment>
					    </discount_categories>
					</fields>
                </initial>
                <configurable>
                	<label>Price rules</label>
                    <sort_order>13</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <expanded>0</expanded>
                    <fields>
	                	<use_variant_simple_price translate="label">
                            <label>Use simple price</label>
                            <comment>Pull simple price from variants instead. Pricing options will not be used in price calculation</comment>
                            <frontend_type>select</frontend_type>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                        </use_variant_simple_price>
					</fields>
                </configurable>
            </groups>
        </price>
    </sections>
</config>